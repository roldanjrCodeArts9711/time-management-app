	"description": "Wonderful app that will help you to be more productive while staying healthy.",
	"scripts": {
		"commit": "git-cz",
		"clean": "gatsby clean",
		"dev": "gatsby develop",
		"prebuild": "yarn clean",
		"build": "gatsby build",
		"start": "npm run dev",
		"lint": "eslint \"**/*.{js,jsx,ts,tsx}\"",
		"format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
		"serve": "yarn clean && yarn build && gatsby serve",
		"cp:sitemap": "cp sitemap.xml public",
		"predeploy": "yarn build --prefix-paths && yarn cp:sitemap",
		"deploy": "gh-pages -d public -r https://$GH_TOKEN@github.com/roldanjr/pomatez.git"
	},
	"dependencies": {
		"babel-plugin-styled-components": "^1.11.1",
		"dotenv": "^8.2.0",
